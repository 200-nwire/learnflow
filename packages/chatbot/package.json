{
  "name": "@amit/chatbot",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    },
    "./components": {
      "import": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsup src/index.ts src/vue.ts --format esm,cjs --dts",
    "test": "vitest run",
    "dev:test": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "vue-renderer-markdown": "^0.0.63-beta.0",
    "socket.io-client": "^4.8.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "tsup": "^8.0.2",
    "vitest": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@types/node": "^22.5.0",
    "eslint": "^9.11.1",
    "vue": "^3.5.11"
  }
}

